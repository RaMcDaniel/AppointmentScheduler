<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SchedulesByContactController">
   <children>
      <AnchorPane layoutY="5.0" prefHeight="600.0" prefWidth="800.0">
         <children>
            <Button fx:id="contactReportExit" layoutX="715.0" layoutY="547.0" mnemonicParsing="false" onAction="#onContactReportExit" text="Exit" />
            <Label layoutX="615.0" layoutY="551.0" text="Return to Menu" />
            <Label fx:id="viewingForLabel" layoutX="267.0" layoutY="14.0" text="Viewing Schedules by Contact">
               <font>
                  <Font size="20.0" />
               </font>
            </Label>
            <TableView fx:id="contactsAppointmentsTable" layoutX="17.0" layoutY="104.0" onSort="#onContactsAppointmentsTable" prefHeight="430.0" prefWidth="766.0">
               <columns>
                  <TableColumn fx:id="contactIDCol" prefWidth="40.0" text="ID" />
                  <TableColumn fx:id="contactTitleCol" prefWidth="74.4000244140625" text="Title" />
                  <TableColumn fx:id="contactDescriptionCol" prefWidth="83.20001220703125" text="Description" />
                  <TableColumn fx:id="contactLocationCol" prefWidth="102.4000244140625" text="Location" />
                  <TableColumn fx:id="contactTypeCol" prefWidth="75.20001220703125" text="Type" />
                  <TableColumn fx:id="contactStartCol" prefWidth="102.39996337890625" text="Start Time" />
                  <TableColumn fx:id="contactEndCol" prefWidth="82.39996337890625" text="End Time" />
                  <TableColumn fx:id="contactCustomerCol" prefWidth="77.5999755859375" text="Customer" />
                  <TableColumn fx:id="contactUserCol" prefWidth="61.59991455078125" text="User" />
                  <TableColumn fx:id="contactContactCol" prefWidth="72.0" text="Contact" />
               </columns>
            </TableView>
            <ComboBox fx:id="chooseUser" layoutX="215.0" layoutY="61.0" onAction="#onChooseUser" prefWidth="150.0" promptText="contacts" />
            <Label layoutX="117.0" layoutY="65.0" text="Choose a contact" />
            <Button fx:id="runUserReport" layoutX="519.0" layoutY="61.0" mnemonicParsing="false" onAction="#onRunUserReport" text="Run Report" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
