<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SchedulesbyUserController">
   <children>
      <Button fx:id="userReportExit" layoutX="715.0" layoutY="547.0" mnemonicParsing="false" onAction="#onUserReportExit" text="Exit" />
      <Label layoutX="615.0" layoutY="551.0" text="Return to Menu" />
      <Label fx:id="viewingForLabel" layoutX="282.0" layoutY="14.0" text="Viewing Schedules by User">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TableView fx:id="usersAppointmentsTable" layoutX="17.0" layoutY="104.0" onSort="#onUsersAppointmentsTable" prefHeight="430.0" prefWidth="766.0">
         <columns>
            <TableColumn fx:id="usersIDCol" prefWidth="40.0" text="ID" />
            <TableColumn fx:id="usersTitleCol" prefWidth="74.4000244140625" text="Title" />
            <TableColumn fx:id="usersDescriptionCol" prefWidth="83.20001220703125" text="Description" />
            <TableColumn fx:id="usersLocationCol" prefWidth="102.4000244140625" text="Location" />
            <TableColumn fx:id="usersTypeCol" prefWidth="75.20001220703125" text="Type" />
            <TableColumn fx:id="usersStartCol" prefWidth="102.39996337890625" text="Start Time" />
            <TableColumn fx:id="userEndCol" prefWidth="82.39996337890625" text="End Time" />
            <TableColumn fx:id="usersCustomerCol" prefWidth="77.5999755859375" text="Customer" />
            <TableColumn fx:id="usersUserCol" prefWidth="61.59991455078125" text="User" />
            <TableColumn fx:id="usersContactCol" prefWidth="72.0" text="Contact" />
         </columns>
      </TableView>
      <ComboBox fx:id="chooseUser" layoutX="215.0" layoutY="61.0" onAction="#onChooseUser" prefWidth="150.0" promptText="choose user" />
      <Label layoutX="117.0" layoutY="65.0" text="Choose a user" />
      <Button fx:id="runUserReport" layoutX="519.0" layoutY="61.0" mnemonicParsing="false" onAction="#onRunUserReport" text="Run Report" />
   </children>
</AnchorPane>
