<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.SchedulesReportController">
   <children>
      <AnchorPane layoutX="10.0" layoutY="10.0" prefHeight="600.0" prefWidth="800.0">
         <children>
            <Label layoutX="292.0" layoutY="14.0" text=" User Schedules Report">
               <font>
                  <Font name="System Bold" size="20.0" />
               </font>
            </Label>
            <TextField fx:id="userScheduleReportTextField" layoutX="18.0" layoutY="95.0" onAction="#onUserScheduleReportTextField" prefHeight="457.0" prefWidth="762.0" />
            <Button fx:id="exit" layoutX="701.0" layoutY="560.0" mnemonicParsing="false" onAction="#onExit" text="Exit" />
            <Label layoutX="265.0" layoutY="59.0" text="Choose a User" />
            <ComboBox fx:id="chooseUserSchedulesReport" layoutX="385.0" layoutY="55.0" onAction="#onChooseUserSchedulesReport" prefWidth="150.0" promptText="choose a user" />
            <Button fx:id="runSchedulesReport" layoutX="594.0" layoutY="55.0" mnemonicParsing="false" onAction="#onRunSchedulesReport" text="run report" />
         </children>
      </AnchorPane>
   </children>
</AnchorPane>
